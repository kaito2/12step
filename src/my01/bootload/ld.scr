/* OUTPUT_FORMAT("elf32-h8300") */
OUTPUT_ARCH(h8300h)
ENTRY("_start")

SECTIONS
{
    /* メモリの先頭版力の配置 */
    . = 0x0;

    /* 割り込みベクタ */
    .vectors : {
        vector.o(.data)
    }

    /* テキスト領域 */
    .text : {
        *(.strings)
        *(.rodata)
        *(.rodata.*)
    }

    /* データ領域 */
    .data : {
        *(.data)
    }

    /* BSS領域 */
    .bss : {
        *(.bss)
        *(COMMON)
    }
}